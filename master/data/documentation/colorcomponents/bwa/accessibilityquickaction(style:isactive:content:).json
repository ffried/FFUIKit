        },
        {
          "inlineContent" : [
            {
              "text" : "The following example shows how to add a quick action to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "pause and resume a workout, with the ",
              "type" : "text"
            },
            {
              "code" : "AccessibilityQuickActionStyle\/prompt",
              "type" : "codeVoice"
            },
            {
              "text" : " style.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "@State var isPaused = false",
            "@State var isQuickActionActive = false",
            "",
            "var body: some View {",
            "    WorkoutView(isPaused: $isPaused)",
            "        .accessibilityQuickAction(style: .prompt, isActive: $isQuickActionActive) {",
            "            Button(isPaused ? \"Resume\" : \"Pause\") {",
            "                isPaused.toggle()",
            "            }",
            "        }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "The following example shows how to add a quick action to",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "play and pause music, with the ",
              "type" : "text"
            },
            {
              "code" : "AccessibilityQuickActionStyle\/outline",
              "type" : "codeVoice"
            },
            {
              "text" : " style.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "@State var isPlaying = false",
            "@State var isQuickActionActive = false",
            "",
            "var body: some View {",
            "    PlayButton(isPlaying: $isPlaying)",
            "        .contentShape(.focusEffect, Circle())",
            "        .accessibilityQuickAction(style: .outline, isActive: $isQuickActionActive) {",
            "            Button(isPlaying ? \"Pause\" : \"Play\") {",
            "                isPlaying.toggle()",
            "            }",
            "        }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
