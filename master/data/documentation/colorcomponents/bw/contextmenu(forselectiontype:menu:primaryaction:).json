    {
      "content" : [
        {
          "anchor" : "return-value",
          "level" : 2,
          "text" : "Return Value",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A view that sets the item-based contextual menu for this",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "view.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    },
    {
      "kind" : "parameters",
      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The identifier type of the items. This must match",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "the selection type of the container that will present the menu.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "itemType"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A view that produces the menu. The parameter is a set",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "of the items the menu should act on. If the set is empty, the menu was",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "activated over the empty area of the selectable container.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "menu"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The action to perform on primary interaction",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "of the items.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "primaryAction"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Use an item-based context menu with a container that supports selection,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "such as a ",
              "type" : "text"
            },
            {
              "code" : "List",
              "type" : "codeVoice"
            },
            {
              "text" : " or ",
              "type" : "text"
            },
            {
              "code" : "Table",
              "type" : "codeVoice"
            },
            {
              "text" : ". The modifier’s closure will be invoked by",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the container to construct a context menu for the current context.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The following example shows a ",
              "type" : "text"
            },
            {
              "code" : "List",
              "type" : "codeVoice"
            },
            {
              "text" : " that supports an empty area menu, a",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "single-item menu, and a multi-item menu.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "List(selection: $selection) {",
            "    \/\/ ...",
            "}",
            ".contextMenu(forSelectionType: ItemID.self) { items in",
            "    if items.isEmpty {",
            "        \/\/ Empty area menu",
            "        Button(\"New Item\") { ... }",
            "    } else if items.count == 1 {",
            "        \/\/ Single-item menu",
            "        Button(\"Copy\") { ... }",
            "        Button(\"Delete\", role: .destructive) { ... }",
            "    } else {",
            "        \/\/ Multi-item menu",
            "        Button(\"Copy\") { ... }",
            "        Button(\"New Folder With Selection\") { ... }",
            "        Button(\"Delete Selected\", role: .destructive) { ... }",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "anchor" : "Primary-Actions",
          "level" : 3,
          "text" : "Primary Actions",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Context menus can be created with a custom primary action. In macOS,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "a single click on a row in a selectable container selects that row,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "and you double click to perform the primary action. In iOS and iPadOS,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "tapping on the row is enough to trigger the primary action. To select",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "a row without performing an action, either enter edit mode or hold",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "shift or command on a keyboard while tapping the row.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "In the example below, double clicking the row on macOS will open a",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "new window viewing the selection.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "@Environment(\\.openWindow) private var openWindow",
            "",
            "List(selection: $selection) {",
            "    \/\/ ...",
            "}",
            ".contextMenu(forSelectionType: ItemID.self) { items in",
            "    if !items.isEmpty {",
            "        Button(\"Open in New Window\") {",
            "            openItemsInWindow(items)",
            "        }",
            "    }",
            "} primaryAction: { items in",
            "    openItemsInWindow(items)",
            "}",
            "",
            "func openItemsInWindow(_ items: Set<ItemID>) {",
            "    for item in items {",
            "        openWindow(value: item)",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "using this modifier without a selectable container as a",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "descendant will result in no menu being shown when the menu gesture",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "activates.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Note",
          "style" : "note",
          "type" : "aside"
        }
      ],
      "kind" : "content"
    }
